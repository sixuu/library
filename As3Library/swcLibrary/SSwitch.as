package  {		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.geom.Rectangle;	import flash.events.Event;		[event (name="change",type="SSwitchEvent")]	public class SSwitch extends MovieClip {				var str="on"		var th		public function SSwitch() {			dragBox.addEventListener(MouseEvent.MOUSE_DOWN,onDown)			th=this			this.value="off"		}		private function onDown(e:MouseEvent){			stage.addEventListener(MouseEvent.MOUSE_UP,onUP)			//stage.addEventListener(MouseEvent.MOUSE_MOVE,onMove)			dragBox.startDrag(false,new Rectangle(-1,0,36,0))			}		private function onUP(e:MouseEvent){			stage.removeEventListener(MouseEvent.MOUSE_UP,onUP)			dragBox.stopDrag()			setBack()			var evt:SSwitchEvent=new SSwitchEvent("change")			evt.value=str			dispatchEvent(evt)					}		private function setBack(){						if(dragBox.x<19){				onmc.visible=false				dragBox.x=-1;				str="off"			}else{				dragBox.x=35;				onmc.visible=true				str="on"			}			offmc.visible=!onmc.visible					}				public function set value(st:String){			if(st=="on"){			  dragBox.x=34			  setBack()			}			if(st=="off"){				dragBox.x=0			    setBack()			}		}		public function get value():String{			return str		}	}	}